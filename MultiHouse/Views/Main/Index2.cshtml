@model List<House>

@{
    Layout = "_LayoutMH";
}

<!DOCTYPE html>

<html>
<head>
  <link rel="canonical" href="https://getbootstrap.com/docs/5.0/examples/album/" />
  <link href="https://getbootstrap.com/docs/5.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous" />

  
  <title></title>
</head>
<body>
<div>
  <main>
    
  
    <div class="album py-5 bg-light center-block-main" >
      <div class="container">

        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">

          @foreach (var house in Model)
          {
            <div class="col">
              <div class="card" >
                <a href="/Main/House/@house.Id" class=" nav-link active">
                  <div>
                    <img src="/img/houses/mainImg/@house.MainImg" width="100%" height="100%"/>

                  </div>
                  
                  <div class="card-body">
                    <p class="card-text">
                      @house.Address
                    </p>

                  </div>
                </a>
              </div>
            </div>
          }

        </div>
      </div>
    </div>
  </main>
</div>
</body>
</html>